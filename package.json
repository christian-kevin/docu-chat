{
  "name": "chat-doc-search",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e env/supabase.env -e env/open-router.env -- next dev",
    "start": "dotenv -e env/supabase.env -e env/open-router.env -- next start",
    "build": "dotenv -e env/supabase.env -e env/open-router.env -- next build",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > types/database.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset"
  },
  "dependencies": {
    "@openrouter/sdk": "^0.3.10",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.49.2",
    "@xenova/transformers": "^2.17.2",
    "ai": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "lucide-react": "^0.468.0",
    "mammoth": "^1.6.0",
    "next": "^15.1.6",
    "openai": "^4.104.0",
    "p-limit": "^7.2.0",
    "pdfjs-dist": "^5.4.449",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.40.1",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.40.1",
    "postcss": "^8.4.49",
    "prettier": "^3.1.1",
    "supabase": "^2.70.4",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
